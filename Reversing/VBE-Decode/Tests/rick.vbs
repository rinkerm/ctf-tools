'Frees temp resources
SUB Close(BYVAL Name)
	ON ERROR RESUME NEXT
	DIM FSO
	SET FSO=CreateObject("Scripting.FileSystemObject")
	WHILE FSO.FileExists(Name)
		CALL FSO.DeleteFile(Name)
		WScript.Sleep 2000
	WEND
	SET FSO=NOTHING
END SUB

'Writes temp resource
SUB Write(BYVAL Name,BYREF Content)
	DIM FSO,File
	SET FSO=CreateObject("Scripting.FileSystemObject")
	SET File = FSO.CreateTextFile(Name,TRUE,FALSE)
	CALL File.Write(Content)
	CALL File.Close()
	SET File=NOTHING
	SET FSO=NOTHING
END SUB

'Runs a temp resource
SUB Run(BYVAL cmd)
	DIM Shell
	SET Shell=CreateObject("WScript.Shell")
	CALL Shell.Exec(cmd)
	SET Shell=NOTHING
END SUB

'Decodes a resource string
FUNCTION Decode(BYVAL str)
	CONST ENC="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/"
	DIM dataLength,sOut,groupBegin
	str=Replace(str,vbCrLf,"")
	str=Replace(str,vbTab,"")
	str=Replace(str," ","")
	dataLength=Len(str)
	IF dataLength MOD 4<>0 THEN
		Err.Raise 1,"Decode","Bad string"
		EXIT FUNCTION
	END IF
	FOR groupBegin=1 TO dataLength STEP 4
		DIM numDataBytes,CharCounter,thisChar,thisData,nGroup,pOut
		numDataBytes=3
		nGroup=0
		FOR CharCounter=0 TO 3
			thisChar=MID(str,groupBegin+CharCounter,1)
			IF thisChar="!" THEN
				numDataBytes=numDataBytes-1
				thisData=0
			ELSE
				thisData=InStr(1,ENC,thisChar,vbBinaryCompare)-1
			END IF
			IF thisData=-1 THEN
				Err.Raise 2,"Decode","Bad character In resource string."
				EXIT FUNCTION
			END IF
			nGroup=64*nGroup+thisData
		NEXT
		nGroup=Hex(nGroup)
		nGroup=STRING(6-Len(nGroup),"0") & nGroup
		pOut= _
			Chr(CBYTE("&H" & MID(nGroup,1,2)))+ _
			Chr(CBYTE("&H" & MID(nGroup,3,2)))+ _
			Chr(CBYTE("&H" & MID(nGroup,5,2)))
		sOut=sOut & Left(pOut,numDataBytes)
	NEXT
	Decode=sOut
END FUNCTION